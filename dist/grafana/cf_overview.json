{
  "title": "CustomField Overview",
  "panels": [
    {
      "type": "graph",
      "title": "API RPS",
      "targets": [
        {"expr": "sum(rate(cf_api_requests_total[1m])) by (path)"}
      ]
    },
    {
      "type": "graph",
      "title": "p95 Latency",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(cf_api_latency_seconds_bucket[5m])) by (le,path))"}
      ]
    },
    {
      "type": "graph",
      "title": "Apply Error Rate",
      "targets": [
        {"expr": "sum(rate(cf_apply_errors_total[5m])) / sum(rate(cf_api_requests_total{path=\"/v1/apply\"}[5m]))"}
      ]
    },
    {
      "type": "barchart",
      "title": "Fields per Table",
      "targets": [
        {"expr": "cf_fields_total"}
      ]
    },
    {
      "type": "graph",
      "title": "Audit Event Rate",
      "targets": [
        {"expr": "sum(rate(cf_audit_events_total[1m]))"}
      ]
    }
  ]
}
